<h1>Log In</h1>
<%= form_for :login do |f| %>
	<div>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>
	<div>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</div>
	<div>
		<%= f.submit "LOGIN" %>
	</div>
<% end %> 

Don't have an account yet? <%= link_to "SIGN UP", users_new_path %>
